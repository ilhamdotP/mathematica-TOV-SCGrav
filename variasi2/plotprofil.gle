begin key
position tr
 hei .4
nobox
line lstyle 1 lwidth 0.07 color blue  text "Mathematica"
line lstyle 2 lwidth 0.07 color black text "Fortran 77"
! text "$l_p=1$ mm"
! text "$\alpha=20$"
