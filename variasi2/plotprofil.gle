size 24 20                              !Sets the size of the paper
set texlabels 1
amove 0.5 10.5
begin graph
        size 12 10                      !Sets the size of the graph
		data "profil-v1.dat"  d10=c1,c2 
		data "profil-vfortran.dat"  d11=c1,c2 
		!yaxis min 0 max 300 dticks 100 hei .5
		!xaxis min 0 max 15 dticks 5 hei .5
        ytitle "$p$ (MeV fm$^{-3}$)"  hei .5
		xtitle "$r$ (km)" hei .5
		d10 line lstyle 1 lwidth 0.07 color blue
		d11 line lstyle 2 lwidth 0.07 color black
		!key pos tl hei 0.4
end graph 
!marker circle msize 0.2 

